{
    "manifestVersion": 1,
    "id": "btdf-tasks-artifact",
    "name": "Fork of Deployment Framework for BizTalk",
    "version": "1.0.0",
    "publisher": "Mikael-Bohlin-Liljekvist",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Deployment Framework for BizTalk Azure DevOps pipeline tasks, with artifact support.",
    "categories": [
        "Build and Release"
    ],
    "tags": [
        "BizTalk",
        "BTDF",
        "MSI",
        "tasks"
    ],
    "icons": {
        "default": "extension-icon.png"        
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/vercellone/vsts-btdf-tasks"
        }
	},
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "galleryFlags": [
        "Public"
    ],
    "files": [
        {
            "path": "TerminateSuspended"
        },
        {
            "path": "BTDFDeploy"
        },
        {
            "path": "BTDFUndeploy"
        },
        {
            "path": "MSIInstall"
        },
        {
            "path": "MSIUninstall"
        }
    ],
    "contributions": [
        {
            "id": "TerminateSuspended(All Artifacts)",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "TerminateSuspended(All Artifacts)"
            }
        },
        {
            "id": "BTDFDeploy(All Artifacts)",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "BTDFDeploy(All Artifacts)"
            }
        },
        {
            "id": "BTDFUndeploy(All Artifacts)",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "BTDFUndeploy(All Artifacts)"
            }
        },
        {
            "id": "MSIInstall(All Artifacts)",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "MSIInstall(All Artifacts)"
            }
        },
        {
            "id": "MSIUninstall(All Artifacts)",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "MSIUninstall(All Artifacts)"
            }
        }
    ]
}